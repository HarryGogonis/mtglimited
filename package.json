    "git-scripts": "^0.2.1",
    "html-webpack-plugin": "^2.25.0",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
