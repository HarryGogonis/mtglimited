    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "react-ultimate-pagination": "^0.9.0",
    "redux": "^3.6.0",
